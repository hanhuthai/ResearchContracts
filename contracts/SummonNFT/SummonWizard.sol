contract SummonWizard is AccessControl, Pausable {
    using SafeERC20 for IERC20;

    bytes32 public constant MODIFY_ENTITIES_ROLE = keccak256("MODIFY_ENTITIES_ROLE");
    uint256 private constant SINGLE_WIZARD_MULTIPLY_COST = 2;

    IERC20 tokenContract;
    IERC721Mint landContract;
    IERC721Mint wizardContract;
    IERC1155 enhancementContract;

    address public tokenBankAddress;
    uint32 public landCooldownPeriod = 172800; // initial 48h
    uint32 public wizardMaturePeriod = 259200; // initial 72h
    uint8 public wizardSoulsDefaultAmount = 6; // amount of souls +1 because it stops at "1"
    uint256[] public summonCosts = [100000, 60000, 37500, 22500, 15000]; // per single Wizard, so for 2x Wizard it will be 2x
    uint256[] public singleUseLandEnhancementIds = [1];
    uint256[] public singleUseWizardEnhancementIds = [0];
    mapping(uint256 => LandCooldown) public landsCooldown;
    mapping(uint256 => Wizard) public wizards;

    struct Wizard {
        uint8 soulsRemainingAmount; //0=uninitialized, 1=no more souls, 6=default souls amount
        uint32 matureTimestamp;
    }

    struct LandCooldown {
        uint32 start;
        uint32 period;
    }

    struct SingleWizardCost {
        uint256 id;
        uint256 cost;
    }

    event Summoned(address receiver, uint256 newWizardId, uint256[] landIds, uint256[] wizardIds, uint256[] ingredientIds, uint256 costSum);
    event SummonedExtra(address receiver, uint256 newWizardId, uint256[] landIds, uint256[] wizardIds, uint256[] ingredientIds);
    event IncreasedWizardSoulAmount(uint256 wizardId, uint8 remainingSoulsAmount);
    event DecreasedWizardSoulAmount(uint256 wizardId, uint8 remainingSoulsAmount);
    event SetLandCooldown(uint256 landId, uint32 period);
    event SetWizardMatureTimestamp(uint256 wizardId, uint32 period);
    event SetSummonCosts(uint256[] _costs);

    constructor (
        IERC20 _tokenAddress,
        IERC721Mint _landContractAddress,
        IERC721Mint _wizardContractAddress,
        IERC1155 _enhancementContractAddress
    ) {
        tokenContract = _tokenAddress;
        landContract = _landContractAddress;
        wizardContract = _wizardContractAddress;
        enhancementContract = _enhancementContractAddress;

        tokenBankAddress = _msgSender();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(MODIFY_ENTITIES_ROLE, _msgSender());
    }

    function summon(
        uint256[] calldata _landIds,
        uint256[] calldata _wizardIds,
        uint256[] calldata _ingredientIds
    ) external whenNotPaused returns (uint256){
        // Input validation
        require(_landIds.length <= 1, "MAX_1_LAND");
        require(_wizardIds.length <= 2, "MAX_2_WIZARDS");
        require(_ingredientIds.length <= 3, "MAX_3_INGREDIENTS");

        uint256 _i;
        uint256 _j;

        // can be negative
        int256 _landNftAmount = 1;
        int256 _wizardNftAmount = 2;

        // Check Ingredients
        for (_i = 0; _i < _ingredientIds.length; _i++) {
            // If single-land given then reduce amount of required wizards
            for (_j = 0; _j < singleUseLandEnhancementIds.length; _j++) {
                if (_ingredientIds[_i] == singleUseLandEnhancementIds[_j]) {
                    _landNftAmount--;
                    break;
                }
            }

            // If single-wizard given then reduce amount of required wizards
            for (_j = 0; _j < singleUseWizardEnhancementIds.length; _j++) {
                if (_ingredientIds[_i] == singleUseWizardEnhancementIds[_j]) {
                    _wizardNftAmount--;
                    break;
                }
            }
        }

        // Validation
        require(_wizardNftAmount >= 0 && _landNftAmount >= 0, "TOO_MANY_SINGLE_WIZARD_OR_LAND_INGREDIENTS");
        require(_landIds.length == uint256(_landNftAmount), "WRONG_LAND_AMOUNT");
        require(_wizardIds.length == uint256(_wizardNftAmount), "WRONG_WIZARDS_AMOUNT_OR_EPHEMERAL_ELIXIRS");
        if (2 == _wizardIds.length) {
            require(_wizardIds[0] != _wizardIds[1], "WIZARDS_CAN_NOT_BE_THE_SAME");
        }

        // Check if last Land cooldown allows to make summoning & sender is Land owner
        if (_landIds.length > 0) {
            require(_msgSender() == landContract.ownerOf(_landIds[0]), "USER_IS_NOT_LAND_OWNER");
            require((landsCooldown[_landIds[0]].start + landsCooldown[_landIds[0]].period) <= uint32(block.timestamp), "LAND_DURING_COOLDOWN");

            // Set a new cooldown start for used land
            landsCooldown[_landIds[0]].start = uint32(block.timestamp);
            landsCooldown[_landIds[0]].period = landCooldownPeriod;
        }

        // Calculate token cost
        uint256 _costSum = _summonCost(_wizardIds);

        // Check if is owner of selected Wizard
        for (_i = 0; _i < _wizardIds.length; _i++) {
            require(_msgSender() == wizardContract.ownerOf(_wizardIds[_i]), "USER_IS_NOT_WIZARD_OWNER");

            // Check if Wizard is matured
            require(_isWizardMatured(_wizardIds[_i]), "WIZARD_IS_NOT_YET_MATURED");

            _decreaseWizardSoulAmount(_wizardIds[_i]);
        }

        // Transfer user's SCRL to our Bank address
        tokenContract.transferFrom(_msgSender(), tokenBankAddress, _costSum);

        // Burn all used ingredients
        if (_ingredientIds.length > 0) {
            _burn(_ingredientIds);
        }

        // Mint a new Wizard for user
        uint256 _newWizardId = wizardContract.mint(_msgSender());

        // Set Wizard mature time
        wizards[_newWizardId].matureTimestamp = uint32((block.timestamp) + uint256(wizardMaturePeriod));

        emit Summoned(_msgSender(), _newWizardId, _landIds, _wizardIds, _ingredientIds, _costSum);

        return _newWizardId;
    }

    function summonExtra(
        address receiver,
        uint256[] calldata _landIds,
        uint256[] calldata _wizardIds,
        uint256[] calldata _ingredientIds
    ) external whenNotPaused onlyRole(MODIFY_ENTITIES_ROLE) returns (uint256){
        // Input validation
        require(_landIds.length <= 1, "MAX_1_LAND");
        require(_wizardIds.length <= 2, "MAX_2_WIZARDS");
        require(_ingredientIds.length <= 3, "MAX_3_INGREDIENTS");

        uint256 _i;
        uint256 _j;

        // can be negative
        int256 _landNftAmount = 1;
        int256 _wizardNftAmount = 2;

        // Check Ingredients
        for (_i = 0; _i < _ingredientIds.length; _i++) {
            // If single-land given then reduce amount of required wizards
            for (_j = 0; _j < singleUseLandEnhancementIds.length; _j++) {
                if (_ingredientIds[_i] == singleUseLandEnhancementIds[_j]) {
                    _landNftAmount--;
                    break;
                }
            }

            // If single-wizard given then reduce amount of required wizards
            for (_j = 0; _j < singleUseWizardEnhancementIds.length; _j++) {
                if (_ingredientIds[_i] == singleUseWizardEnhancementIds[_j]) {
                    _wizardNftAmount--;
                    break;
                }
            }
        }

        // Validation
        require(_wizardNftAmount >= 0 && _landNftAmount >= 0, "TOO_MANY_SINGLE_WIZARD_OR_LAND_INGREDIENTS");
        require(_landIds.length == uint256(_landNftAmount), "WRONG_LAND_AMOUNT");
        require(_wizardIds.length == uint256(_wizardNftAmount), "WRONG_WIZARDS_AMOUNT_OR_EPHEMERAL_ELIXIRS");
        if (2 == _wizardIds.length) {
            require(_wizardIds[0] != _wizardIds[1], "WIZARDS_CAN_NOT_BE_THE_SAME");
        }

        // Mint a new Wizard for user
        uint256 _newWizardId = wizardContract.mint(receiver);

        // Set Wizard mature time
        wizards[_newWizardId].matureTimestamp = uint32((block.timestamp) + uint256(wizardMaturePeriod));

        emit SummonedExtra(receiver, _newWizardId, _landIds, _wizardIds, _ingredientIds);

        return _newWizardId;
    }

    function summonCostPerWizard(uint256[] calldata _wizardIds) external view returns (SingleWizardCost[] memory _costsStruct, uint256[] memory _costsArray){
        _costsStruct = new SingleWizardCost[](_wizardIds.length);
        _costsArray = new uint256[](_wizardIds.length);
        uint256[] memory _singleWizardIdArray = new uint256[](1);
        uint256 _singleWizardCost;
        for (uint256 _i = 0; _i < _wizardIds.length; _i++) {
            // Check if Wizard exists
            try wizardContract.ownerOf(_wizardIds[_i]){
                // Cost for only one Wizard per entry
                _singleWizardIdArray[0] = _wizardIds[_i];
                _singleWizardCost = _summonCost(_singleWizardIdArray) / SINGLE_WIZARD_MULTIPLY_COST;
            } catch {
                // No Wizard, so cost 0
                _singleWizardCost = 0;
            }

            // Put into output struct
            _costsStruct[_i] = SingleWizardCost(
                _wizardIds[_i],
                _singleWizardCost
            );
            _costsArray[_i] = _singleWizardCost;
        }
    }

    function summonCostForWizardsWithItems(uint256[] calldata _wizardIds, uint256[] calldata _ingredientIds) external view returns (uint256){
        require(_wizardIds.length <= 2, "MAX_2_WIZARDS");
        require(_ingredientIds.length <= 3, "MAX_3_INGREDIENTS");

        uint256 _i;
        uint256 _j;
        int256 _wizardNftAmount = 2;
        for (_i = 0; _i < _ingredientIds.length; _i++) {
            for (_j = 0; _j < singleUseWizardEnhancementIds.length; _j++) {
                if (_ingredientIds[_i] == singleUseWizardEnhancementIds[_j]) {
                    _wizardNftAmount--;
                    break;
                }
            }
        }

        require(_wizardNftAmount >= 0, "TOO_MANY_SINGLE_WIZARDS");
        require(_wizardIds.length == uint256(_wizardNftAmount), "WRONG_WIZARDS_AMOUNT_OR_EPHEMERAL_ELIXIRS");
        if (2 == _wizardIds.length) {
            require(_wizardIds[0] != _wizardIds[1], "WIZARDS_CAN_NOT_BE_THE_SAME");
        }

        return _summonCost(_wizardIds);
    }

    function setTokenBankAddress(address _tokenBankAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        tokenBankAddress = _tokenBankAddress;
    }

    function setLandCooldownPeriod(uint32 _landCooldownPeriod) external onlyRole(DEFAULT_ADMIN_ROLE) {
        landCooldownPeriod = _landCooldownPeriod;
    }

    function setSingleUseLandEnhancementIds(uint256[] calldata _ids) external onlyRole(DEFAULT_ADMIN_ROLE) {
        delete singleUseLandEnhancementIds;
        for (uint256 i = 0; i < _ids.length; i++) {
            singleUseLandEnhancementIds.push(_ids[i]);
        }
    }

    function setSingleUseWizardEnhancementIds(uint256[] calldata _ids) external onlyRole(DEFAULT_ADMIN_ROLE) {
        delete singleUseWizardEnhancementIds;
        for (uint256 i = 0; i < _ids.length; i++) {
            singleUseWizardEnhancementIds.push(_ids[i]);
        }
    }

    function setWizardSoulsDefaultAmount(uint8 _souls) external onlyRole(DEFAULT_ADMIN_ROLE) {
        wizardSoulsDefaultAmount = _souls;
    }

    function setSummonCosts(uint256[] calldata _costs) external onlyRole(DEFAULT_ADMIN_ROLE) {
        delete summonCosts;
        for (uint256 i = 0; i < _costs.length; i++) {
            require(_costs[i] > 0, "COST_MUST_BE_ABOVE_0");
            summonCosts.push(_costs[i]);
        }

        emit SetSummonCosts(_costs);
    }

    function setSingleLandCooldownPeriod(uint256 _landId, uint32 _period) external whenNotPaused onlyRole(MODIFY_ENTITIES_ROLE) {
        landsCooldown[_landId].period = _period;

        emit SetLandCooldown(_landId, _period);
    }

    function setGlobalWizardMaturePeriod(uint32 _wizardMaturePeriod) external onlyRole(DEFAULT_ADMIN_ROLE) {
        wizardMaturePeriod = _wizardMaturePeriod;
    }

    function setSingleWizardMatureTimestamp(uint256 _wizardId, uint32 _timestamp) external whenNotPaused onlyRole(MODIFY_ENTITIES_ROLE) {
        wizards[_wizardId].matureTimestamp = _timestamp;

        emit SetWizardMatureTimestamp(_wizardId, _timestamp);
    }

    function isWizardMatured(uint256 _wizardId) external view returns (bool){
        return _isWizardMatured(_wizardId);
    }

    function _isWizardMatured(uint256 _wizardId) internal view returns (bool){
        return uint32(block.timestamp) >= wizards[_wizardId].matureTimestamp;
    }

    function initializeWizardSouls(uint256 _wizardId) external onlyRole(MODIFY_ENTITIES_ROLE) {
        _setupWizard(_wizardId);
    }

    // Returns in "human form", so real amount of souls (so 5x instead of 6x)
    function getWizardSoulsRemainingAmount(uint256 _wizardId) external view returns (uint8) {
        // Wizard not in mapping
        if (0 == wizards[_wizardId].soulsRemainingAmount) {
            // Check if Wizard exists
            try wizardContract.ownerOf(_wizardId){
                return wizardSoulsDefaultAmount - 1;
            } catch {
                return 0;
            }
        } else {
            // Wizard in mapping
            // Decrease by 1 (6-1=5) to return a real amount of souls externally
            return wizards[_wizardId].soulsRemainingAmount - 1;
        }
    }

    function increaseWizardSoulAmount(uint256 _wizardId) external whenNotPaused onlyRole(MODIFY_ENTITIES_ROLE) {
        // Set 5 souls if not performed any summoning yet
        if (0 == wizards[_wizardId].soulsRemainingAmount) {
            // 5 souls but we stop at 1, so 5+1=6
            wizards[_wizardId].soulsRemainingAmount = wizardSoulsDefaultAmount;
        }

        wizards[_wizardId].soulsRemainingAmount++;

        emit IncreasedWizardSoulAmount(_wizardId, wizards[_wizardId].soulsRemainingAmount);
    }

    function decreaseWizardSoulAmount(uint256 _wizardId) external whenNotPaused onlyRole(MODIFY_ENTITIES_ROLE) {
        _decreaseWizardSoulAmount(_wizardId);

        emit DecreasedWizardSoulAmount(_wizardId, wizards[_wizardId].soulsRemainingAmount);
    }

    function _decreaseWizardSoulAmount(uint256 _wizardId) internal {
        _setupWizard(_wizardId);

        require(wizards[_wizardId].soulsRemainingAmount > 1, "SOULLESS_WIZARD");
        wizards[_wizardId].soulsRemainingAmount--;
    }

    function _setupWizard(uint256 _wizardId) internal {
        // Set 5 souls if not performed any summoning yet
        if (0 == wizards[_wizardId].soulsRemainingAmount) {
            // 5 souls but we stop at 1, so 5+1=6
            wizards[_wizardId].soulsRemainingAmount = wizardSoulsDefaultAmount;
        }
    }

    function _summonCost(uint256[] memory _wizardIds) internal view returns (uint256){
        uint256 _i;
        uint256 _cost = 0;
        uint256 _costSum = 0;
        uint256 _tmpWizardSoulsRemainingAmount;

        for (_i = 0; _i < _wizardIds.length; _i++) {
            // Calculate cost
            if (0 == wizards[_wizardIds[_i]].soulsRemainingAmount) {
                // 5 souls but we stop at 1, so 5+1=6
                _tmpWizardSoulsRemainingAmount = wizardSoulsDefaultAmount;
            } else {
                _tmpWizardSoulsRemainingAmount = wizards[_wizardIds[_i]].soulsRemainingAmount;
            }

            require(_tmpWizardSoulsRemainingAmount > 1, "SOULLESS_WIZARD");
            _tmpWizardSoulsRemainingAmount --;

            if (summonCosts.length < _tmpWizardSoulsRemainingAmount) {
                // If amount of souls is out of price boundaries, e.g. more souls than usual
                // then set cost equal minimal cost for all extra souls
                _cost = summonCosts[summonCosts.length - 1];
            } else {
                _cost = summonCosts[_tmpWizardSoulsRemainingAmount - 1];
            }

            _costSum += _cost;
        }

        // Cost of single-use wizards
        if (1 == _wizardIds.length) {
            // One single-use wizard used: double used wizard cost
            _costSum = _costSum * SINGLE_WIZARD_MULTIPLY_COST;
        } else if (0 == _wizardIds.length) {
            // Two single-use wizard used: 2x minimal cost
            _costSum += summonCosts[summonCosts.length - 1] * 2;
        }

        _costSum = _costSum * 10 ** 18;

        return _costSum;
    }

    function _burn(uint256[] calldata _ingredientIds) internal {
        uint256[] memory _burnValues = new uint256[](_ingredientIds.length);
        uint256 _i;
        for (_i = 0; _i < _ingredientIds.length; _i++) {
            _burnValues[_i] = 1;
        }
        enhancementContract.burnBatch(_msgSender(), _ingredientIds, _burnValues);
    }

    function returnAccidentallySent(IERC20 _tokenToSent) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 _amount = _tokenToSent.balanceOf(address(this));
        _tokenToSent.safeTransfer(_msgSender(), _amount);
    }

    function pause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }
}